    <div>
      <a class="hiddenanchor" id="signin"></a>

      <div class="login_wrapper">
        <div class="animate form login_form">
          <section class="login_content">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
              <h1>Login Form</h1>
              <div>
                <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control", placeholder: "E-mail" %>
              </div>
              <div>
                <%= f.password_field :password, autocomplete: "current-password", class: "form-control", placeholder: "Senha" %>

              </div>
              <div>
                <%= f.submit "Log in", class: "btn btn-default submit" %>

                <a class="reset_pass" href="#">Lost your password?</a>
              </div>

              <div class="clearfix"></div>

              <div class="separator">
                <%= render "users/shared/links", class: "to_register" %>

                <div class="clearfix"></div>
                <br />

                <div>
                  <h1><i class="fa fa-paw"></i> Gentelella Alela!</h1>
                  <p>Â©2016 All Rights Reserved. Gentelella Alela! is a Bootstrap 4 template. Privacy and Terms</p>
                </div>
              </div>
            <% end %>
          </section>
        </div>
      </div>
    </div>

